{
  "name": "grunt-subgrunt",
  "version": "1.3.0",
  "description": "Run sub-projects' grunt tasks.",
  "repository": {
    "type": "git",
    "url": "git://github.com/tusbar/grunt-subgrunt.git"
  },
  "scripts": {
    "test": "eslint . && grunt test"
  },
  "keywords": [
    "gruntplugin",
    "subgrunt",
    "subproject",
    "submodule"
  ],
  "author": {
    "name": "Bertrand Marron",
    "url": "https://github.com/tusbar"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tusbar/grunt-subgrunt/issues"
  },
  "dependencies": {
    "async": "^2.1.2",
    "glob": "^7.0.0"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "eslintIgnore": [
    "**/node_modules/**"
  ],
  "devDependencies": {
    "eslint": "^4.13.0",
    "eslint-config-xo-space": "^0.17.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-nodeunit": "^1.0.0",
    "load-grunt-tasks": "^3.4.1"
  }
}
